cps  ;
  w @cs
  s (st,sd,sst,std)=""
  s D1="\",D2="#",D3="!",D4="|",dvt="VT420"
  s cps="Completed"
  s rtn=$q(^["DEMO"]ultn) i rtn="" s cps="unknown status" g dis   ; check for processor node
  s rtn=$q(^["DEMO"]ultn("stop")) s:rtn="" cps="Running"    ; check for processor stop
  s st=$p(^["DEMO"]ultn("start","trans"),",",2),st=$zt(st)  ; get control processor start time
  s sd=$p(^["DEMO"]ultn("start","trans"),","),sd=$zd(sd)    ; get control processor start date
  i cps="Running" s xsp=0 d dis f  q:xsp  d
  . s total=$g(^["DEMO"]uresint("total")) 
  . d POS(8,25) W "Total Records Processed "_total  
  . d POS(11,25) w "Press Enter to Continue " r r:3 i $t s xsp=1
  . i $q(^["DEMO"]ultn("stop"))'="" s cps="Completed" w @cs g gst 
gst 
  q:cps="Running" 
  s sst=$p(^["DEMO"]ultn("stop","trans"),",",2),sst=$zt(sst)   ; get control processor stop time
  s std=$p(^["DEMO"]ultn("stop","trans"),","),std=$zd(std)    ; get control processor stop date
dis  ;
  W !,UDL,?80,NOR
  w !,?25,"Incremental Extract Status"
  w !!,?5,"Start Date/Time",?30,"Stop Date/Time",?60,"Extract Status"
  w !!,?5,sd,?16,st,?30,std,?41,sst,?60,cps
  s total=$g(^["DEMO"]uresint("total"))
  d POS(8,25) W "Total Records Processed "_total   
  d POS(10,0) W UDL,?80,NOR
  q:cps="Running"
  w !?25,"Press Enter to Continue "  r r
  q        
POS(X,Y) ; Position cursor to x --> row and Y --> columns 
 N LDV S LDV=$I 
POS1 I $D(%TRM(LDV,"POS"))=0 Q:'$$INIT^TRM("POS") 
 U LDV W %TRM(LDV,"POS",1)_(X+1)_%TRM(LDV,"POS",2)_(Y+1)_%TRM(LDV,"POS",3)  
 S $X=$S(+Y>0:Y-1,1:0),$Y=$S(+X>0:+X,1:0)  
 Q 
job ; 
  d POS(20,0) W UDL,?80,NOR 
  d POS(5,15) w msg,!
  s (p1,p2,p3,que)=0 k jtab D GET^%ZJTAB(.jtab) s j="",i=0 f z=0:0 s j=$o(jtab(j)) q:j=""!(que)  d 
  . s zjob=$$PAR^%ZJOB(j) 
  . w ?5,"." s i=i+1 i i>70 w ! s i=0 
  . i $p(zjob,"\",6)="ultn" s id=$p(zjob,"\"),p3=1 w !,?25,"Stopping ultn" s rtn=$zu(4,id)  
  . i $p(zjob,"\",6)=pid d  i p2=1 s que=1 q
  .. w !,?25,"Stopping "_pid 
  .. s pid=$p(zjob,"\") s rtn=$zu(4,pid) s p1=1 
  . i $p(zjob,"\",6)=pid1 d  i p1=1 s que=1 q
  .. w !,?25,"Stopping "_pid1 
  .. s pid1=$p(zjob,"\") s rtn=$zu(4,pid1) s p2=1
  i (p1'=1)&&(p2'=1)&&(p3'=1) d POS(18,25) w "No Processes where found - Stop Aborted "
  d POS(21,25) w "Press Enter to Continue " r r
 q  



