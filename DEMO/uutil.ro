dism  ;
  s sd=$h,st=$p(sd,",",2),sd=$p(sd,","),sd=$zd(sd),st=$zt(st)
  B "l+" 
  s erio="c:/extract/file" 
  o erio:"wns":2 
  u erio w "WARNING!  Daily Incremental Processor has been disabled on ",sd," at",st
  c erio 
  s rmail=^["DEMO"]umail
  f i=1:1 s em=$p(rmail,",",i) q:em=""  d 
  . s dat="mail -s "_qt_"DEMO  Incremental Processor Disabled"_qt_" "_em_" < /c:extract/file" 
  . s rtn=$zf(-1,dat) 
  q
